<%= form_for @artist do |f| %>
  <p>
  <%= f.label :name, "Artist Name" %>
  <%= f.text_field :name, :autofocus => true %>
  </p>
  <p>
  <%= f.label :image %>
  <%= f.text_field :image %>
  </p>
  <p>
  <%= f.label :dob, "Date of Birth" %>
  <%= f.text_field :dob, :placeholder => 'YYYY/MM/DD' %>
  </p>
    <p>
  <%= f.label :nationality %>
  <%= f.text_field :nationality %>
  </p>
  <p>
  <%= f.label :movement %>
  <%= f.text_field :movement %>
  </p>
  <%= f.submit %>
<% end %>