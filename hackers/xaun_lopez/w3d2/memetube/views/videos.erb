<% @videos.each do |video| %>
  <div class="video">
    <h3>
      <a href="/memetube/<%= video['id'] %>">
        <%= video['title'] %>
      </a>
    </h3>
    <div class="embed">
      <iframe width="420" height="315" src="//www.youtube.com/embed/<%= video['url'].split(/&|=/).last %>" frameborder="0" allowfullscreen></iframe>
    </div>
  </div>
<% end %>




